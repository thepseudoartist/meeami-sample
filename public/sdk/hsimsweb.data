[NETWORKPROFILES]
# mprofiles define the network preferred media information
# currently codec list, ptime, application specific maximum bandwidth in kbps (not used)
# and RTCP RS, RR bandwitdth values in bps can be configured
# The codec used here should be configured in default codecs list also (for codec param details)
# If both rtcp bandwidths are zeros will disable the rtcp flow
# Valid Nwk types are WIFI, WIMAX, ETHERNET, CELLDATA, CELLDATA-2G, VPN, BLUETOOTH, LTE, CS 
# CELLDATA for 3G(HSPA, CDMA2000-1x, EVDO) and CELLDATA-2G for 2G(GPRS/EDGE) networks
# Entry should be in the following form 
# mprofile=<nw_type>; <codec>: <ptime>; <AS_BW>; <RS_BW>, <RR_BW>; <SERVICES>
# mprofile=<nw_type>; <codec, codec, codec>; <ptime>; <as_bw>; <rs_bw> <rr_bw>; <service, service, service>
# Example: mprofile=WIFI; PCMU, PCMA: 20; 64; 800, 2400;CALL, IM

mprofile=ETHERNET; AMR-WB,iLBC,PCMU,AMR; 20; 64; 800,2400;
mprofile=WIFI; AMR-WB,iLBC,PCMU,AMR; 20; 64; 800,2400;
mprofile=CELLDATA; AMR-WB,iLBC,PCMU,AMR; 20; 64; 800,2400;IM,CALL,PRESENCE
mprofile=CELLDATA-2G;AMR-WB,iLBC,PCMU,AMR; 20; 64; 0,0;IM
mprofile=LTE; AMR-WB,iLBC,PCMU,AMR; 20; 64; 800,2400;

# Codec rate of g723  1 for 5.3, 2 for 6.3
codecrate=1

# DTMF type
# 0 - DTMF Disabled
# 1 - INBAND PASS THROUGH MODE
# 2 - INBAND,PCMU MODE
# 3 - INBAND,PCMA MODE
# 4 - 2833 RELAY
# 5 - SIP INFO
# 6 - SIP INFO CISCO
# If DTMF is disabled then DTMF detection will be disabled,
# which is required for call features like conference through SLIC.
# if endpoint type is other than sound-card, DTMF cannot be disabled
dtmftype=4

# Redundancy levels as per RFC 2198
# 0 - disable 
redundancy-levels=6

# Dynamic payload type for redundancy
red-payload-type=120

# iLBC mode
iLBC-mode=30

# Minimum number packets to be received successively with same invalid 
# pt (i.e., other than negotiated) before triggering codec change indication
codecchangelimit=1

# PT value of G7221-24
g7221-24pt=122

# PT value of G7221-32
g7221-32pt=123

# PT value of ILBC
ilbcpt=104

# PT value of G726-16
g726-16pt=96

# PT value of G726-24
g726-24pt=97

# PT value of G726-32
g726-32pt=98

# PT value of G726-40
g726-40pt=99

# PT value of DTMF(tele-event)
# Dynamic PT for dtmf is filled in stack
dtmfpt=101

# PT value of AMR-NB
amrnb-pt=105

# AMR codec octet align (Valid values are 0 and 1)
amrnb-octet-align=0

# AMR codec mode set (valid values are any combination of values from 0 to 7 separated with comma)
amrnb-modeset=0,1,2,3,4,5

# AMR codec mode change neighbor (valid values are 0 and 1)
amrnb-mode-change-neighbor=0

# AMR codec mode change neighbor (valid values are 1 and 2)
# Currently this param is not supported
amrnb-mode-change-capability=2

# AMR codec CRC (valid values are 0 and 1)
# Used only if octet-align is non zero
amrnb-crc=0

# AMR codec robust sorting (valid values are 0 and 1)
# Used only if octet-align is non zero
amrnb-robust-sorting=0

# AMR codec interleaving (used only if value is Non zero value)
# Used only if octet-align is non zero
amrnb-interleaving=0

# AMR codec channels (supported values are 1 and 2. currently 1 is supported)
amrnb-channels=1

# PT value of AMR-WB
amrwb-pt=102

# AMR codec octet align (Valid values are 0 and 1)
amrwb-octet-align=0

# AMR codec mode set (valid values are any combination of values from 0 to 7 separated with comma)
amrwb-modeset=0,1,2,3,4

# AMR codec mode change neighbor (valid values are 0 and 1)
amrwb-mode-change-neighbor=0

# AMR codec mode change neighbor (valid values are 1 and 2)
# Currently this param is not supported
amrwb-mode-change-capability=2

# AMR codec CRC (valid values are 0 and 1)
# Used only if octet-align is non zero
amrwb-crc=0

# AMR codec robust sorting (valid values are 0 and 1)
# Used only if octet-align is non zero
amrwb-robust-sorting=0

# AMR codec interleaving (used only if value is Non zero value)
# Used only if octet-align is non zero
amrwb-interleaving=0

# AMR codec channels (supported values are 1 and 2. currently 1 is supported)
amrwb-channels=1

# PT value of EVRCB0
evrcb0pt=106

# PT value of EVRCB1
evrcb1pt=107

# PT value of EVRCB
evrcbpt=108

# Receive mode for EVRCB0 (valid values 0,1,2,3,4,5,6,7)
evrcb0-recvmode=0

# Send mode for EVRCB0 (valid values 0,1,2,3,4,5,6,7)
evrcb0-sendmode=0

# Max rate for EVRCB0
evrcb0-maxrate=4

# Min rate for EVRCB0
evrcb0-minrate=1

# Fixed rate for EVRCB1
# 0 - 0.5
# 1 - 1
evrcb1-fixedrate=0

# Receive mode for EVRCB1 (valid values 0,1,2,3,4,5,6,7)
# If evrcb1 fixed rate is 1 then 0 - 6 are valid for mode 7 is not accepted
evrcb1-recvmode=0

# Send mode for EVRCB1 (valid values 0,1,2,3,4,5,6,7)
# If evrcb1 fixed rate is 1 then 0 - 6 are valid for mode 7 is not accepted
evrcb1-sendmode=0

# Receive mode for EVRCB (valid values 0,1,2,3,4,5,6,7)
evrcb-recvmode=0

# Send mode for EVRCB (valid values 0,1,2,3,4,5,6,7)
evrcb-sendmode=0

# Max rate for EVRCB
evrcb-maxrate=4

# Min rate for EVRCB
evrcb-minrate=1

# EVRCB maximum interleave
# maximum interleave length supported is zero.
evrcb-maxinterleave=0

# comma separated list of modes supported in order of preference
# valid values are 0-8, '*' implies supports all modes 
#ex: values as ‘3,* ‘  all modes are set and 3 is preferred mode
speexnb-mode=3 

# valid values are 0, 1, 
# if codec specific vad is used then  vbr=vad is set
speexnb-vbr=0

# comma separated list of modes supported
# valid values are 0-10, if '*' implies supports all modes
speexwb-mode=8

# valid values are 0, 1, 
# if vadtype=2  vbr=vad is sent
speexwb-vbr=0

# comma separated list of modes supported
# valid values are 0-10, if '*' implies supports all modes
speexuwb-mode=8

# valid values are 0, 1, 
# if codec specific vad is used then  vbr=vad is set
speexuwb-vbr=0

# valid values are 0, 1, 
# if codec specific vad is used then  vbr=vad is set
ambe-modeset=0

# Forward error correction option
# 0 – disable
# 1 – multiple stream
# 2 – using redundancy
# Currenlty only multiple stream is supported
fec-opt=0

# FEC schemes supported
fec-schemes=ulpfec reed-solomon-fec parityfec

# FEC one level only
# 0 – more than one level of FEC protection is used
# 1 - only one level of FEC protection is used
fec-onelevelonly=0

# Max number of Encoded Symbols (K)
fec-max-k=12

# Max number of Encoded and Parity Symbols (N)
# Should be greater then max number of encoded symbols(fec-max-k)
fec-max-n=15

# OPUS Max capture rate
opus-maxcapturerate=16000

# OPUS Max avg bits per second of the session
opus-maxavgbitrate=20000

# OPUS Constant bitrate 'on' or 'off'
opus-cbr=0

# OPUS In-band FEC 'on' or 'off'
opus-useinbandfec=0

# OPUS Algorithm complexity
opus-complexity=8

[[RTT]]

# Number of redundant T140 blocks in an RTP packet
# 0 - redundancy disabled
rtt-redundancy-levels=2

# Max Tx data buffering time before sending to network in milli seconds
rtt-txbuftime=300

# Max wait time of Rx data before issuing to APP in milli seconds
rtt-rxbuftime=1000

# Wait time since no data is received from remote party, to conclude, it is non-responsive
rtt-remote-idletimeout=300

# Rx capability of receiving max number of characters per second
rtt-cps=30

[[RTP]]

# RTP TX packet size
rtptxpacketsize=20

# Allowed maximum percentage of RTP packet loss.
# The rate is calculated over a few minutes.
# If loss rate exceeds this value, call will be disconnected
# On EDGE, there could be very high packet loss
rtppacketlosslimit=100

# Allowed maximum number of consecutive ICMP destination
# unreachable responses before disconnecting the call 
rtperrorlimit=30

# The start port number from which RTP can use UDP ports
rtpportbase=49000

# The end port number upto which RTP can use UDP ports
rtpportlimit=49050

# ToS(Type of Service) field for IP packets containing RTP messages, valid values are 0-63
rtp-tos=184

# Option to choose sockets, during IP address transition
# 1 - Continue on sockets created on old IP
# 2 - Use sockets created on new IP
# 3 - Use neigther
rtp-lipc-sendopt=2

# Bitmap to set RTCP option where bits used as
# 0x 0  0  0  0  0  0  0  0
#         VH  VA       AH AA
# 0 -  0x00 - disable RTCP
# 1 -  0x01 - RTCP for Audio session active(AA)
# 2 -  0x02 - RTCP for Audio session in hold(AH)
# 16 - 0x10 - RTCP for Video session active(VA)
# 32 - 0x20 - RTCP for Video session hold(VH)
rtcp-option=51

# RTCP interval
rtcp-intvl=5

# RTCP-FB parameters bitmap
# 0   - 0x0000 - disable RTCP feedback
# 1   - 0x0001 - RTCP ack param rpsi
# 2   - 0x0002 - RTCP ack param app
# 4   - 0x0004 - RTCP nack param pli
# 8   - 0x0008 – RTCP nack param sli
# 16  - 0x0010 – RTCP nack param rpsi
# 32  - 0x0020 - RTCP nack param app
# 64  - 0x0040 – RTCP nack generic
# 128 – 0x0080 – RTCP ccm param fir
# 256 – 0x0100 - RTCP ccm param tmmbr
# 512 – 0x0200 - RTCP ccm param tstr
# 1024– 0x0400 - RTCP ccm param vbcm /* Currently not supported */
rtcpfb-opt=128

# RTCP-FB trr-int in seconds
rtcpfb-trrintvl=5

# RTCP-FB tmmbr max packet rate value
rtcpfb-smaxpr=

# SDP maximum packetisation time
sdpmaxpacketisationtime=100


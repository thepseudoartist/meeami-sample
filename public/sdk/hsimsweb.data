[[MSRP]]

# MSRP transport type valid value TCP/TLS
msrp-xport-type=TCP

# Port on which MSRP socket is listening for incoming requests. 
# [RFC 4975 suggests listening port to be 2855]
msrp-tcp-port=2855

# Ports dedicated for MSRP [default range 15500-15999]

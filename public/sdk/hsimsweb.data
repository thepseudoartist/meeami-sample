<?xml version = "1.0" encoding = "UTF-8"?>
<ncm-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns = "http://www.hellosoft.com/ncm-config.xsd">
   
   <!-- ncm config contains list of adapter elements -->
   <!-- adapter element contains all elements related to an adapter information
        The 'type' attribute specifies type of adapter
        example: wlan, WIMAX, cell, cell-data, HSPA, PPP, BT, IKE, LTE -->

   <!-- The elements like ss(signal strength), snr, ber, ecio contains entry, scan,
        exit, return limits as attributes -->

   <!-- The element if-name-list contains adapter names separated by comma.
        The names which are matched here will be treated as corresponding
        adapter type -->

   <!-- management element is optional. It contains sub elements like
        auto-handoff, manage-opt etc-->

   <!-- The element static-if contains adapter name as [name = "lte0"] and an adapter IP address.
	The names which are matched here will be treated as available in system and will be added
	to adapter list. If the static-if is also available in system, The interface should be added
	in ignore-if-name-list. -->

   <!-- ignore-if-name-list is a separate element which is parallel to adapter element and
	holds interface name list. These adapter names will be ignored even through these
	are available in system -->

   <adapter type = "wlan">
      <ss entry = "70" return = "60" scan = "50" exit = "40"/>
      <snr entry = "35" return = "30" scan = "25" exit = "20"/>
      <management>
         <auto-handoff>1</auto-handoff>
         <manage-opt>1</manage-opt>
         <auto-select-if>1</auto-select-if>
	 <if-name/>
         <scan-list-view>3</scan-list-view>
	 <enterprise-profile-name/>
      </management>
      <!-- In some Android phones, WiFi adapter has name "eth0" -->
      <if-name-list>
         wifi, wi-fi, wireless, 80211, 802.11, TNET, tiwlan, NPME\JEDI11, wlan
      </if-name-list>
   </adapter>

   <adapter type = "wimax">
      <ss entry = "40" return = "30" scan = "20" exit = "10"/>
      <if-name-list> wimax, wibro </if-name-list>
   </adapter>

   <adapter type = "cell">
      <ss entry = "40" return = "30" scan = "20" exit = "10"/>
      <ber entry = "4" return = "5" scan = "6" exit = "7"/>
      <ecio entry = "1" return = "3" scan = "5" exit = "7"/>
   </adapter>

   <adapter type = "cell-data">
      <ss entry = "40" return = "30" scan = "20" exit = "10"/>
      <snr entry = "4" return = "3" scan = "2" exit = "1"/>
      <ecio entry = "1" return = "3" scan = "5" exit = "7"/>
      <if-name-list>
         edge, gprs, cdma, evdo, ev-do, evdv, ev-dv, cell, rmnet
      </if-name-list>
   </adapter>

   <adapter type = "hspa">
      <if-name-list> hspa, hsdpa, hsupa </if-name-list>
   </adapter>

   <adapter type = "ppp">
      <if-name-list> ppp </if-name-list>
   </adapter>

   <adapter type = "bt">
      <if-name-list> bluetooth </if-name-list>
   </adapter>

   <adapter type = "ike">
      <if-name-list> safenet, safe-net </if-name-list>
   </adapter>

   <adapter type = "lte">
      <if-name-list>VoLTE-Network, rmmet_data</if-name-list>
      <!--<static-if name = "lte0">192.168.41.23</static-if> -->
   </adapter>

   <!--<ignore-if-name-list>GCTLTE1, L50LTE1</ignore-if-name-list> --> 
   <ignore-if-name-list></ignore-if-name-list>
   <apn>
      <type>1</type>
      <name>geosecure.motolab.com</name>
   </apn>
</ncm-config>

